  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "type": "module",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.15.4",
    "@types/react": "^18.0.0",
    "@changesets/cli": "^2.27.7",
    "prettier": "^3.3.3",
    "tsup": "^8.2.4",
    "typescript": "^5.6.2",
    "vitest": "^2.0.5"
  },
